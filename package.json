    "@rollup/plugin-replace": "^4.0.0",
    "accepts": "^1.3.8",
    "amex-jest-preset": "^6.1.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-amex": "^4.0.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "danger": "^11.0.7",
    "eslint": "^7.32.0",
    "eslint-config-amex": "^14.2.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jest-dom": "^3.9.4",
    "expect": "^28.1.1",
    "find-up": "^5.0.0",
    "fs-extra": "^10.1.0",
    "https-proxy-agent": "^5.0.1",
    "husky": "^8.0.1",
    "if-node-version": "^1.1.1",
