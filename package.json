    "@americanexpress/fetch-enhancers": "^1.1.3",
    "@americanexpress/lumberjack": "^1.1.4",
    "@americanexpress/one-app-bundler": "^6.15.0",
    "@americanexpress/one-app-ducks": "^4.4.1",
    "@americanexpress/one-app-router": "^1.2.1",
    "@americanexpress/vitruvius": "^2.0.2",
    "@fastify/express": "^2.0.1",
    "@fastify/helmet": "^9.1.0",
    "@fastify/rate-limit": "^7.3.0",
    "abort-controller": "^3.0.0",
    "body-parser": "^1.20.0",
    "bytes": "^3.1.2",
    "cacheable-lookup": "^6.0.4",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.23.3",
    "cors": "^2.8.5",
    "create-shared-react-context": "^1.0.5",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.2.0",
    "helmet": "^6.0.0",
    "holocron": "^1.3.0",
    "holocron-module-route": "^1.3.0",
    "if-env": "^1.0.4",
    "immutable": "^4.1.0",
    "ip": "^1.1.8",
    "joi": "^17.6.0",
    "lean-intl": "^4.2.2",
    "matcher": "^4.0.0",
    "opossum": "^6.4.0",
    "opossum-prometheus": "^0.3.0",
