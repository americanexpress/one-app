    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@americanexpress/one-app-bundler": "^6.21.8",
    "babel-preset-amex": "^4.0.3"
  }
}
